<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RecommendActivity">

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="30dp"
        tools:layout_editor_absoluteX="-159dp"
        tools:layout_editor_absoluteY="-283dp"
        >
        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="返回"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="533dp" />

        <com.github.xiaofeidev.round.RoundFrameLayout
            android:id="@+id/frame6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="5dp"
            app:rd_radius="20dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/elderly5"
                    android:layout_width="141dp"
                    android:layout_height="163dp"
                    android:layout_margin="0dp"
                    android:padding="0dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/elderly1"

                    />

                <LinearLayout
                    android:layout_width="206dp"
                    android:layout_height="177dp"
                    android:background="@color/teal_700"
                    android:gravity="center"
                    android:orientation="vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toEndOf="@+id/elderly5"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.495">

                    <ImageView
                        android:id="@+id/arrow1"
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:src="@drawable/ic_action_right"
                        android:clickable="true"
                        />

                    <TextView
                        android:id="@+id/textView15"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="太平盛世颐享人生年金保险"
                        android:textColor="@android:color/primary_text_dark"
                        android:textSize="14dp" />
                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </com.github.xiaofeidev.round.RoundFrameLayout>

        <com.github.xiaofeidev.round.RoundFrameLayout
            android:id="@+id/frame2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="5dp"
            app:rd_radius="20dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/elderly2"
                    android:layout_width="141dp"
                    android:layout_height="163dp"
                    android:layout_margin="0dp"
                    android:padding="0dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/elderly2" />

                <LinearLayout
                    android:layout_width="210dp"
                    android:layout_height="160dp"
                    android:background="@color/teal_700"
                    android:gravity="center"
                    android:orientation="vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toEndOf="@+id/elderly2"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="1.0">

                    <ImageView
                        android:id="@+id/arrow2"
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:clickable="true"
                        android:src="@drawable/ic_action_right" />

                    <TextView
                        android:id="@+id/textView18"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="太平金世集合型团体养老保\n障理产品"
                        android:textColor="@android:color/primary_text_dark"
                        android:textSize="14dp" />
                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </com.github.xiaofeidev.round.RoundFrameLayout>

        <com.github.xiaofeidev.round.RoundFrameLayout
            android:id="@+id/frame3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="5dp"
            app:rd_radius="20dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/elderly3"
                    android:layout_width="141dp"
                    android:layout_height="163dp"
                    android:layout_margin="0dp"
                    android:padding="0dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/elderly3" />

                <LinearLayout
                    android:layout_width="210dp"
                    android:layout_height="160dp"
                    android:background="@color/teal_700"
                    android:gravity="center"
                    android:orientation="vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toEndOf="@+id/elderly3"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="1.0">

                    <ImageView
                        android:id="@+id/arrow3"
                        android:clickable="true"
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:src="@drawable/ic_action_right" />

                    <TextView
                        android:id="@+id/textView19"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="太平金世智盈集合型团体养老保障管理产品"
                        android:textColor="@android:color/primary_text_dark"
                        android:textSize="14dp" />
                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </com.github.xiaofeidev.round.RoundFrameLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>